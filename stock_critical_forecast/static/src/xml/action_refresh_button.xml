<?xml version="1.0" encoding="UTF-8"?>
<template xml:space="preserve">
  <t t-extend="ListView.buttons">
    <t t-jquery="div.o_list_buttons" t-operation="append">
      <button t-if="widget.modelName == 'stock.critical_forecast'" class="btn btn-primary oe_action_button" type="button" accesskey="f">Refresh</button>
      <div class="o_list_selection_box container_last_updated_on" t-if="widget.modelName == 'stock.critical_forecast'">
        <i>
          <strong>Last Updated On: <span class="container_last_updated_date"/>
          </strong>
        </i>
      </div>
    </t>
  </t>
</template>
